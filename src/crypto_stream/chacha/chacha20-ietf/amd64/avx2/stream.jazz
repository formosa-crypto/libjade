export fn jade_stream_chacha_chacha20_ietf_amd64_avx2_xor(reg u64 output plain len nonce key) -> reg u64
{
  reg u64 r;
  __chacha_xor_avx2(output, plain, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}

