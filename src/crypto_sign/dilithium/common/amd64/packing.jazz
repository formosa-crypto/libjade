	?{}, i = #set0_64();
	while(i < Li2_polydeg / 8) {
		?{}, j = #set0_64();
		while (j < 8) {
			addr = #LEA_64(8*i + j);
			v32 = t0[(int) addr];
			t0_term = v32;
			v32 = max_t0;
			v32 -= t0_term;
			t[(int) j] = v32;
			j += 1;
		}

		// Get ready.
		// Here be typos.
		// TODO: Check if there are indeed no typos

		//r[13*i+ 0]  =  t[0];

		v32 = t[0];
