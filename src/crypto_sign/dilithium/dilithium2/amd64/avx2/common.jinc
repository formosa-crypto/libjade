inline
fn expandA(stack u8[32] rho) -> stack u32[Li2_k * Li2_l * Li2_polydeg] {
	stack u32[Li2_k * Li2_l * Li2_polydeg] matrix;
	matrix = expandA_aligned(rho);
	return matrix;
}